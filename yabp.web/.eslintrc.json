{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // off
        "require-jsdoc": "off",
        "react/prop-types": "off",
        "linebreak-style": "off",
        "react/react-in-jsx-scope": "off",

        // warn
        "valid-jsdoc": "warn",
        "no-unused-vars": "warn",
        "object-curly-spacing": "warn",
        "max-len": "warn"
 
    }
}

{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "valid-jsdoc": "warn",
        "require-jsdoc" : "off",
        "react/prop-types" : "off",
        "linebreak-style" : "off",
        "no-unused-vars" : "warn",
        "object-curly-spacing" : "warn",
        "max-len" : "warn"
    }
}
